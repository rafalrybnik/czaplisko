{
  "name": "czaplisko-cms",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 2137",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "db:push": "dotenv -- prisma db push",
    "db:seed": "dotenv -- tsx prisma/seed.ts",
    "db:studio": "dotenv -- prisma studio",
    "db:migrate": "dotenv -- prisma migrate dev",
    "db:reset": "dotenv -- prisma migrate reset"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "isbot": "^5.1.34",
    "jsonwebtoken": "^9.0.3",
    "nuxt": "^4.3.0",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "sharp": "^0.34.5",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.16.0",
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.21.0"
  }
}
